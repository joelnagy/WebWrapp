<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.www.amp.webwrapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>NAME</name>
    <description>
        DESCRIPTION
    </description>
    <author email="hello@example.com" href="https://www.example.com">
        AUTHOR
    </author>
    <content src="https://www.example.com/path" />
    <preference name="StatusBarBackgroundColor" value="#STATUSBARBACKGROUNDCOLOR" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="AppendUserAgent" value="WebWrapp 1.0" />
    <preference name="Orientation" value="portrait" />
    <access origin="*" />
    <access origin="cdvfile://*" />
    <allow-intent href="data:*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <splash density="port-ldpi" src="res/screen/android/splash.9.png" />
        <splash density="port-mdpi" src="res/screen/android/splash.9.png" />
        <splash density="port-hdpi" src="res/screen/android/splash.9.png" />
        <splash density="port-xhdpi" src="res/screen/android/splash.9.png" />
        <splash density="port-xxhdpi" src="res/screen/android/splash.9.png" />
        <splash density="port-xxxhdpi" src="res/screen/android/splash.9.png" />
        <icon density="ldpi" src="res/icon/android/icon-ldpi-36.png" />
        <icon density="mdpi" src="res/icon/android/icon-mdpi-48.png" />
        <icon density="hdpi" src="res/icon/android/icon-hdpi-72.png" />
        <icon density="xhdpi" src="res/icon/android/icon-xhdpi-96.png" />
        <icon density="xxhdpi" src="res/icon/android/icon-xxhdpi-144.png" />
        <icon density="xxxhdpi" src="res/icon/android/icon-xxxhdpi-192.png" />
        <edit-config file="app/src/main/AndroidManifest.xml" mode="merge" target="/manifest/application">
            <application android:icon="@drawable/ic_launcher" />
        </edit-config>
        <resource-file src="res/icon/android/ic_launcher_background.png" target="app/src/main/res/drawable/ic_launcher_background.png" />
        <resource-file src="res/icon/android/ic_launcher_foreground.png" target="app/src/main/res/drawable/ic_launcher_foreground.png" />
        <resource-file src="res/icon/android/ic_launcher.xml" target="app/src/main/res/drawable/ic_launcher.xml" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="144" width="144" src="icon-144.png" />
        <icon height="72" width="72" src="icon-72-hdpi.png" width="72" />
    </platform>
    <platform name="windows">
    </platform>
    <platform name="wp8">
    </platform>
    <universal-links>
        <host name="www.example.com" scheme="https" />
        <host name="amp.example.org" scheme="https" />
    </universal-links>
    <allow-navigation href="*" />
    <allow-navigation href="data:*" />
    <allow-navigation href="https://*/*" />
    <allow-navigation hap-rule="yes" href="https://www.example.com/*" />
    <allow-navigation hap-rule="yes" href="https://*.cdn.ampproject.org/*" />
    <plugin name="cordova-plugin-hostedwebapp" spec="^0.3.1" />
    <plugin name="cordova-plugin-network-information" spec="^2.0.1" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.1" />
    <plugin name="cordova-plugin-themeablebrowser" spec="^0.2.17" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-universal-links-plugin" spec="^1.2.1" />
    <plugin name="cordova-plugin-file" spec="^6.0.1" />
    <engine name="osx" spec="^4.0.1" />
    <engine name="android" spec="^7.0.0" />
    <engine name="browser" spec="^4.1.0" />
</widget>
